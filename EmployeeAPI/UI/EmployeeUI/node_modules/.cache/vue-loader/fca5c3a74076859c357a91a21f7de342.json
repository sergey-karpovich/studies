AAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;AAClC","file":"C:/git/projects/PetProject/studies/EmployeeAPI/UI/EmployeeUI/src/pages/employees/EmployeeList.vue","sourceRoot":"","sourcesContent":["<template>\r\n    <div>\r\n        <base-dialog :show=\"!!error\" title=\"An error occurred!\" \r\n        @close=\"handleError\">\r\n        <p>{{ error }}</p>\r\n        </base-dialog>\r\n        <section>\r\n            <!-- <base-card v-if=\"isCreating\" title=\"Create New Employee\" >   \r\n                <employee-form @close=\"closeCreateForm\" @submit-employee=\"submitEmployee\"></employee-form>\r\n            </base-card> -->\r\n            <!-- <coach-filter @change-filter=\"setFilters\"></coach-filter> -->\r\n            <base-button link to=\"/employee/register\" >New Employee</base-button>\r\n        </section>\r\n        <section> \r\n            <base-card>\r\n            <div class=\"controls\">\r\n                \r\n            </div> \r\n            <div v-if=\"isLoading\">\r\n                <base-spinner></base-spinner>\r\n            </div>\r\n                <ul v-else-if=\"hasEmployees\">\r\n                    <employee-item v-for=\"employee in employees\" \r\n                    :key=\"employee.EmployeeId\" \r\n                    :id=\"employee.EmployeeId\" \r\n                    :first-name=\"employee.firstName\" \r\n                    :last-name=\"employee.lastName\" \r\n                    :description=\"employee.description\"\r\n                    :rate=\"employee.rate\"\r\n                    :areas=\"employee.areas\"\r\n                    :birth-date=\"employee.birthDate\"\r\n                    :hire-date=\"employee.hireDate\"\r\n                    :phone-number=\"employee.phoneNumber\"\r\n                    :PhotoFileName=\"employee.photoPath\"\r\n                    @deleteEmployee=\"deleteEmployee\"\r\n                    ></employee-item>\r\n                </ul>\r\n                <h3 v-else>No employee found.</h3>\r\n            </base-card>\r\n        </section>     \r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport EmployeeItem from '../../components/employees/EmployeeItem.vue';\r\n\r\nexport default {\r\n    components: {\r\n        EmployeeItem,\r\n        \r\n    },    \r\n    data(){\r\n        return {\r\n            isLoading: false,\r\n            error: null,\r\n            \r\n        }\r\n    },\r\n    computed: {  \r\n        employees(){\r\n            return this.$store.getters['employee/employees']\r\n        } ,\r\n        hasEmployees(){\r\n            return !this.isLoading && this.employees.length>0;\r\n        },\r\n        \r\n    },\r\n    methods: {\r\n        async loadEmployee(){\r\n            this.isLoading = true;\r\n            try{\r\n                await this.$store.dispatch('employee/loadEmployees');\r\n            } catch (error){\r\n                this.error = error.message || 'Something went wrong!';\r\n            }\r\n            this.isLoading=false\r\n        },\r\n        \r\n        handleError() {\r\n            this.error = null;\r\n        },\r\n       \r\n        async deleteEmployee(id){\r\n            this.isLoading = true;\r\n            try{\r\n                await this.$store.dispatch('employee/deleteEmployee',id);\r\n            } catch (error){\r\n                this.error = error.message || 'Something went wrong!';\r\n            }\r\n            this.isLoading=false\r\n        }\r\n    },\r\n    created(){\r\n        this.loadEmployee();\r\n    }\r\n}\r\n</script>\r\n \r\n<style scoped>\r\nul {\r\n    list-style: none;\r\n    margin: 0;\r\n    padding: 0;\r\n}\r\n\r\n.controls {\r\n    display: flex;\r\n    justify-content: space-between;\r\n}\r\n</style>\r\n"]}]}