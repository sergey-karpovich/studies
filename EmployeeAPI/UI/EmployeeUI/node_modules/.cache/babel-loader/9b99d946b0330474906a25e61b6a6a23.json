n            }\r\n            this.isLoading=false\r\n        }\r\n    },\r\n    created(){\r\n        this.loadEmployee();\r\n    }\r\n}\r\n</script>\r\n \r\n<style scoped>\r\nul {\r\n    list-style: none;\r\n    margin: 0;\r\n    padding: 0;\r\n}\r\n\r\n.controls {\r\n    display: flex;\r\n    justify-content: space-between;\r\n}\r\n</style>\r\n"]}]}